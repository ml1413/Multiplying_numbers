<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:backgroundTint="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginHorizontal="@dimen/margin_16dp"
    app:cardCornerRadius="@dimen/radius_card_view_8dp"
    app:strokeColor="@color/black"
    android:layout_marginTop="@dimen/margin_16dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_16dp"
        android:orientation="vertical">


        <include
            android:id="@+id/label"
            layout="@layout/include_tv_label_item_for_result" />

        <include
            android:id="@+id/includeItemTab0"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab1"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab2"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab3"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab4"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab5"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab6"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab7"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab8"
            layout="@layout/include_tv_answer_item_for_result" />

        <include
            android:id="@+id/includeItemTab9"
            layout="@layout/include_tv_answer_item_for_result" />
    </LinearLayout>
</com.google.android.material.card.MaterialCardView>